    "type": "git",
    "url": "git+https://github.com/chingu-voyage4/Geckos-Team-12.git"
  },
  "keywords": [
    "mern",
    "react",
    "express",
    "node",
    "mongodb",
    "grow",
    "plant",
    "food"
  ],
