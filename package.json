{
  "name": "simpleseed",
  "version": "0.1.0",
  "description": "Grow anything simply",
  "main": "index.js",
  "scripts": {
    "install": "(cd client && npm install) && (cd server && npm install)",
    "start":
      "concurrently \"cd client && npm start\" \"cd server && npm start\"",
    "build":
      "concurrently \"cd client && npm run build\" \"cd server && npm run build\"",
    "start:prod": "cd server && npm start:prod",
    "heroku-postbuild": "npm run build"
  },
